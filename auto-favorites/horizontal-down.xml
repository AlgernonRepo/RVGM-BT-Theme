<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<!-- HIDDE ELEMENTS -->
		<text name="sys_label, sys_info" extra="true">
			<visible>false</visible>
		</text>
		<image name="info-bg" extra="true">
			<visible>false</visible>
		</image>	
		<video name="videoPreview" extra="true">
			<visible>false</visible>
		</video>

		<!-- Coupa Sprite -->
		<image name="info" extra="true">
			<origin>0 0</origin>
			<pos>0.045 0.058</pos>
			<path>./info-horizontal-down.png</path>
		</image>
				
		<!-- Princess Peach Sprite -->
		<image name="console" extra="true">
			<origin>0 0</origin>
			<pos>0.078 0.565</pos>
			<zIndex>14</zIndex>
		</image>
		
		<!-- Sytem AVATAR -->
		<image name="avatar" extra="true">
			<pos>0.3655 0.1965</pos>
			<rotation>90</rotation>
		</image>
		
	</view>
</theme>
