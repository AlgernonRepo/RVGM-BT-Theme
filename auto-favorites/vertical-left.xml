<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<!-- HIDDE ELEMENTS -->
		<text name="sys_label, sys_info" extra="static">
			<visible>false</visible>
		</text>
		<image name="info-bg" extra="static">
			<visible>false</visible>
		</image>	
		<video name="videoPreview" extra="static">
			<visible>false</visible>
		</video>

		<!-- Coupa Sprite -->
		<image name="info" extra="static">
			<origin>1 0</origin>
			<pos>0.98 0.258</pos>
		</image>
		
		<!-- Princess Peach Sprite -->
		<image name="console" extra="static">
			<origin>0 0</origin>
			<pos>0.078 0.565</pos>
		</image>
		
		<!-- Sytem AVATAR -->
		<image name="avatar" extra="static">
			<pos>0.3655 0.1965</pos>
			<rotation>90</rotation>
		</image>
		
	</view>
</theme>
